<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jesseniph.paladinsapp.MainActivity">



    <ImageButton
        android:id="@+id/AndroxusBtn"
        android:onClick="goToAndroxus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/androxus_button"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="2dp" />

    <ImageButton
        android:id="@+id/BariksBtn"
        android:onClick="goToBariks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bariksbtn"
        app:layout_constraintLeft_toRightOf="@+id/AndroxusBtn"
        android:layout_marginLeft="2dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="2dp"
        app:layout_constraintTop_toTopOf="@+id/AndroxusBtn" />

    <ImageButton
        android:id="@+id/BombKngBtn"
        android:onClick="goToBombKing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bombkingbtn"
        app:layout_constraintLeft_toRightOf="@+id/BariksBtn"
        android:layout_marginLeft="0dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="2dp"
        app:layout_constraintTop_toTopOf="@+id/BariksBtn"
        android:layout_marginTop="0dp" />

    <ImageButton
        android:id="@+id/BuckBtn"
        android:onClick="goToBuck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/buckbtn"
        app:layout_constraintLeft_toRightOf="@+id/BombKngBtn"
        android:layout_marginLeft="2dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="2dp"
        app:layout_constraintTop_toTopOf="@+id/BombKngBtn" />

    <ImageButton
        android:id="@+id/CassieBtn"
        android:onClick="goToCassie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/cassie_button"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="97dp" />

    <ImageButton
        android:id="@+id/DrogozBtn"
        android:onClick="goToDrogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/drogobtn"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="96dp"
        android:layout_marginTop="97dp" />

    <ImageButton
        android:id="@+id/EvieBtn"
        android:onClick="goToEvie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/evie_button"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="194dp"
        android:layout_marginTop="97dp" />

    <ImageButton
        android:id="@+id/FernandoBtn"
        android:onClick="goToFernando"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/fernando_button"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="292dp"
        android:layout_marginTop="97dp" />

    <ImageButton
        android:id="@+id/GrohkBtn"
        android:onClick="goToGrohk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/grohk_button"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="189dp" />

    <ImageButton
    android:id="@+id/GroverBtn"
        android:onClick="goToGrover"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/grover_button"
    tools:layout_constraintTop_creator="1"
    tools:layout_constraintLeft_creator="1"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_marginLeft="98dp"
    android:layout_marginTop="189dp" />

    <ImageButton
        android:id="@+id/MakoaBtn"
        android:onClick="goToMakoa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/makoa_button"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="194dp"
        android:layout_marginTop="189dp" />



</android.support.constraint.ConstraintLayout>
